{
  "name": "shuvroroy/laravel-translation-manager",
  "description": "Manage Laravel Translations",
  "keywords": [
    "laravel",
    "translations",
    "translator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Shuvro Roy",
      "email": "shuvro.nsu.cse@gmail.com"
    }
  ],
  "require": {
    "php": ">=7",
    "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x",
    "illuminate/translation": "5.5.x|5.6.x|5.7.x|5.8.x",
    "symfony/finder": "~3.0|~4.0",
    "tanmuhittin/laravel-google-translate": "^0.11.0"
  },
  "autoload": {
    "psr-4": {
      "ShuvroRoy\\TranslationManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ShuvroRoy\\TranslationManager\\ManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
